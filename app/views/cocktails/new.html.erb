<h1 class="text-center">New Cocktails</h1>

<br>
<br>
<div class="container">
  <%= simple_form_for [@cocktail] do |f| %>
    <%= f.input :name %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.submit "Add Cocktail", class: "btn btn-primary" %>
  <% end %>
</div>
<br>
